package com.shachar.first;

public class Post {
	private int id;
	private String email;
	private String postText;

	public Post(int id, String email, String postText, int replayFor, String postSubTopic) {
		this.id = id;
		this.email = email;
		this.postText = postText;
		this.replayFor = replayFor;
		this.postSubTopic = postSubTopic;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public String getEmail() {
		return email;
	}

	public void setEmail(String email) {
		this.email = email;
	}

	public String getPostText() {
		return postText;
	}

	public void setPostText(String postText) {
		this.postText = postText;
	}

	public int getReplayFor() {
		return replayFor;
	}

	public void setReplayFor(int replayFor) {
		this.replayFor = replayFor;
	}

	public String getPostSubTopic() {
		return postSubTopic;
	}

	public void setPostSubTopic(String postSubTopic) {
		this.postSubTopic = postSubTopic;
	}

	private int replayFor;
	private String postSubTopic;

}
