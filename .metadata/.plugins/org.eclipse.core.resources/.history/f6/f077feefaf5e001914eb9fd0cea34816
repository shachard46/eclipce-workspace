<%
	try {
		Class.forName("com.mysql.jdbc.Driver").newInstance();

		Connection con = DriverManager.getConnection("jdbc:mysql://127.0.0.1:3306/db", "root", "");
		Statement st = con.createStatement(ResultSet.TYPE_SCROLL_INSENSITIVE, ResultSet.CONCUR_UPDATABLE);




		String mySQL = "SELECT * FROM members";


		ResultSet oRS = st.executeQuery(mySQL);



		oRS.beforeFirst();
	} catch (Exception e) {


		System.out.println("Error in connecting");

	}
	String[] userInfo = new String[8];
	userInfo[1] = request.getParameter("username");
	userInfo[2] = request.getParameter("password");
	userInfo[3] = request.getParameter("email");
	userInfo[4] = request.getParameter("compType");
